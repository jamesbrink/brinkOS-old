_pkgname='brinkOS Icons'
pkgname=brinkOS-icons
pkgver=1.0.0
pkgrel=1
pkgdesc='Icons for brinkOS (Originally Apricity-OS Icons)'
arch=(any)
license=(GPL)
url="https://github.com/jamesbrink/brinkOS"
depends=()
install=brinkOS-icons.install

package() {
	mkdir -p "${pkgdir}/usr/share/icons"
	cp -rf "${srcdir}/brinkOS Icons" "${pkgdir}/usr/share/icons"
}
